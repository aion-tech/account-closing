<odoo>
    <data>
        <record id="marco_gs1_nomenclature" model="barcode.nomenclature">
            <field name="name">MARCO GS1 Nomenclature</field>
            <field name="is_gs1_nomenclature">true</field>
        </record>
        <!-- Creazione di una nuova regola per Internal Locations con la regex specificata -->
         <record id="barcode_rule_gs1_92" model="barcode.rule">
            <field name="name">Internal Location</field>
            <field name="barcode_nomenclature_id" ref="marco_gs1_nomenclature"/>
            <field name="sequence">510</field>
            <field name="encoding">gs1-128</field>
            <field name="pattern">(92)([!"%-/0-9:-?A-Z_a-z]{0,90})</field>
            <field name="type">location</field>
            <field name="gs1_content_type">alpha</field>
        </record>
    </data>
</odoo>
