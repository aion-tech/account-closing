<odoo>
    <data>
    <!--
    Imposte utilizzate nei vari tipi di inversione contabile (TD):
     _________________________________________________________________________________________________________________________
    | Tipo Documento                              | Imposte Purchase                      | Imposte Sale                      |
    |_____________________________________________|_______________________________________|___________________________________|
    | TD16 - Integrazione Reverse Interno         | l10n_it_marco.1_tax_22itd16a_purchase | l10n_it_marco.1_tax_22itd16a_sale |
    |                                             | l10n_it_marco.1_tax_22itd16b_purchase | l10n_it_marco.1_tax_22itd16b_sale |
    |                                             | l10n_it_marco.1_tax_22itd16c_purchase | l10n_it_marco.1_tax_22itd16c_sale |
    |_____________________________________________|_______________________________________|___________________________________|
    | TD17 - Integrazione Servizi UE              | l10n_it_marco.1_tax_22itd17a_purchase | l10n_it_marco.1_tax_22itd17a_sale |
    |_____________________________________________|_______________________________________|___________________________________|
    | TD17 - Autofatture Servizi Extra UE         | l10n_it_marco.1_tax_22atd17a_purchase | l10n_it_marco.1_tax_22atd17a_sale |
    |_____________________________________________|_______________________________________|___________________________________|
    | TD18 - Integrazione Acquisto CEE Beni       | l10n_it_marco.1_tax_22itd18a_purchase | l10n_it_marco.1_tax_22itd18a_sale |
    |_____________________________________________|_______________________________________|___________________________________|
    | TD19 - Integrazione/Autofattura Art. 17 c.2 | l10n_it_marco.1_tax_22itd19a_purchase | l10n_it_marco.1_tax_22itd19a_sale |
    |                                             | l10n_it_marco.1_tax_22atd19a_purchase | l10n_it_marco.1_tax_22atd19a_sale |
    |_____________________________________________|_______________________________________|___________________________________|
    -->
        <!-- INTEGRAZIONE REVERSE INTERNO - TD16 -->
        <record id="reverse_interno_td16" model="account.rc.type">
            <field name="name">INTEGRAZIONE REVERSE INTERNO - TD16</field>
            <field name="method">selfinvoice</field>
            <field name="fiscal_document_type_id" ref="l10n_it_fiscal_document_type.14"/>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="partner_type">other</field>
            <field name="journal_id" ref="l10n_it_marco_extra.account_journal_VENAF"/>
            <field name="payment_journal_id" ref="l10n_it_marco_extra.account_journal_RICRC"/>
            <field name="transitory_account_id" ref="l10n_it_marco.1_22TRAUFA"/>
            <field name="tax_ids" eval="[(5, 0, 0),
                (0, 0, {
                    'purchase_tax_id': ref('l10n_it_marco.1_tax_22itd16a_purchase'),
                    'sale_tax_id': ref('l10n_it_marco.1_tax_22itd16a_sale'),
                }),
                (0, 0, {
                    'purchase_tax_id': ref('l10n_it_marco.1_tax_22itd16b_purchase'),
                    'sale_tax_id': ref('l10n_it_marco.1_tax_22itd16b_sale'),
                }),
                (0, 0, {
                    'purchase_tax_id': ref('l10n_it_marco.1_tax_22itd16c_purchase'),
                    'sale_tax_id': ref('l10n_it_marco.1_tax_22itd16c_sale'),
                }),
            ]"/>
        </record>

