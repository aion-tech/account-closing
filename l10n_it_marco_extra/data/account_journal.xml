<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <!-- Registri di tutte le banche e casse -->
        <!-- Registro Intesa BCI - Rete Comit | Conto principale per acquisti e vendite -->
        <record id="account_journal_LICOMI" model="account.journal">
            <field name="name">Intesa Sanpaolo</field>
            <field name="code">ISP</field>
            <field name="type">bank</field>
            <field name="default_account_id" ref="l10n_it_marco.1_04LICOMI"/>
            <field name="payment_sequence">True</field>
            <field name="sequence">19</field>
        </record>
        <!-- 
        Registro Banca Nazionale del Lavoro | 
        Utilizzato come banca di appoggio per clienti estero che usano BNL come intermediario
        -->
        <record id="account_journal_LIBNL0" model="account.journal">
            <field name="name">Banca Nazionale del Lavoro</field>
            <field name="code">BNL</field>
            <field name="type">bank</field>
            <field name="default_account_id" ref="l10n_it_marco.1_04LIBNL0"/>
            <field name="payment_sequence">True</field>
            <field name="sequence">21</field>
        </record>
        <!-- Registro Cassa Contanti -->
        <record id="account_journal_LICASS" model="account.journal">
            <field name="name">Cassa Contanti</field>
            <field name="code">CASSA</field>
            <field name="type">cash</field>
            <field name="default_account_id" ref="l10n_it_marco.1_04LICASS"/>
            <field name="payment_sequence">True</field>
            <field name="sequence">20</field>
        </record>
        <!-- Registro Intesa Valutario US$ 
        Conto italiano con sede a Brescia in valuta dollaro:
        Alcuni clienti non possono pagare direttamente sul nostro conto Intesa New York,
        quindi abbiamo un conto per fare da intermediario (occasionale tipo per BUGSHAN)
        -->
        <record id="account_journal_LICOM1" model="account.journal">
            <field name="name">Intesa Valutario USD</field>
            <field name="code">IVUS</field>
            <field name="type">bank</field>
            <field name="default_account_id" ref="l10n_it_marco.1_04LICOM1"/>
            <field name="payment_sequence">True</field>
            <field name="sequence">27</field>
        </record>
        <!-- Registro Intesa New York in US$ 
        Questo è invece con sede a New York ed è il conto principale in Dollari, 
        dove noi facciamo i bonifici anche a fornitori esteri
        -->
        <record id="account_journal_LICOM2" model="account.journal">
            <field name="name">Intesa New York in USD</field>
            <field name="code">INYUS</field>
            <field name="type">bank</field>
            <field name="default_account_id" ref="l10n_it_marco.1_04LICOM2"/>
            <field name="payment_sequence">True</field>
            <field name="sequence">26</field>
        </record>
