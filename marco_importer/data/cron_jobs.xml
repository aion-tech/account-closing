<odoo>
    <!-- Cron per items_quant -->
    <record id="ir_cron_import_items_quant" model="ir.cron">
        <field name="name">Import Data for items_quant</field>
        <field name="model_id" ref="model_marco_importer"/> <!-- TransientModel -->
        <field name="state">code</field>
        <field name="code">model.create({'select_all': False}).import_data_in_background('items_quant')</field>
        <field name="user_id" ref="base.user_root"/> <!-- Eseguito come amministratore -->
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="active">False</field>
    </record>

